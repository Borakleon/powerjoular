-s dir
--name powerjoular
--license GPL-3.0-only
--version 0.1.0
--architecture x86_64
--description "PowerJoular is a command line software to monitor, in real time, the power consumption of software and hardware components."
--url "https://www.noureddine.org/research/joular/powerjoular"
--maintainer "Adel Noureddine <adel.noureddine@univ-pau.fr>"

x86_64/powerjoular=/usr/bin/powerjoular ../systemd/powerjoular.service=/etc/systemd/system/powerjoular.service
